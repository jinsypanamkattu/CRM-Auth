
{
    "version": 2,
    "builds": [
      {
        "src": "Server/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "Client/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "Client/build" }
      }
    ],
    "routes": [
      { "src": "/Server/(.*)", "dest": "/Server/index.js" },
      { "src": "/(.*)", "dest": "/Client/build/index.html" }
    ]
  }
  